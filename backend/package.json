    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "firebase-admin": "^13.4.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
